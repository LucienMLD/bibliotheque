<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <%= simple_form_for @booking do |f| %>
            <%= f.input :book, collection: @bookings, :label_method => :book, prompt: "Selectionnez un livre"%>
            <%= f.button :submit, class: "btn btn-default" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
